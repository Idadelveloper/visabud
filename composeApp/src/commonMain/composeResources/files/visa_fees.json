  "visaFeeStructure": {
    "baseApplicationFee": {
      "definition": "Primary charge for visa application processing",
      "variableBy": ["destination_country", "origin_nationality", "visa_category", "visa_type", "processing_location"],
      "example": "Australian 186 Visa: AUD 5,500 for main applicant + AUD 2,750 per dependent"
    },
    "dependentFees": {
      "definition": "Charges for spouse/partners and dependent children",
      "variableBy": ["dependent_type", "dependent_age", "destination_country"],
      "dependentTypes": ["spouse", "de_facto_partner", "child_under_18", "child_18_23_fulltime_student"]
    },
    "processingFeeSurcharges": {
      "definition": "Additional charges on top of base fee for enhanced/expedited service",
      "types": ["express_processing", "priority_service", "super_priority_service"]
    },
    "auxiliaryServiceFees": {
      "definition": "Charges for additional required or optional services",
      "serviceCategories": [
        "biometric_collection",
        "medical_examination",
        "english_language_testing",
        "document_certification",
        "translation_services",
        "police_clearance",
        "health_insurance_surcharge"
      ]
    }
  },

