      "byHostCountry": "Filter by host country",
      "byCity": "Filter by city name",
      "byServices": "Filter by visa types offered",
      "byDistanceRadius": "Filter by distance in km",
      "byOperatingHours": "Filter by currently open/closed"
    }
  },

  "commonQueryExamples": [
    {
      "userQuery": "Find nearest Australian embassy",
      "executionSteps": [
        "Get user's current coordinates (latitude, longitude)",
        "Query: embassies WHERE representingCountry = 'AUS' AND isPrimaryMission = true",
        "Sort by distance using Haversine formula",
        "Return top 1 result with direction and distance"
      ]
    },
    {
      "userQuery": "Where can I get a US work visa near me?",
      "executionSteps": [
        "Get user coordinates",
        "Query: missions WHERE visaTypes includes 'work' AND representingCountry = 'USA'",
        "Sort by proximity (within 50 km)",
        "Return sorted list with visa processing times, requirements, appointment links"
      ]
    },
    {
      "userQuery": "Show me all visa processing locations in Delhi",
      "executionSteps": [
        "Query: missions WHERE city = 'Delhi'",
        "Filter by missions that offer visa services",
        "Sort by mission type (embassy first, then consulates, agencies)",
        "Return with contact info, hours, visa types"
      ]
    },
    {
      "userQuery": "Which embassy is open right now in my city?",
      "executionSteps": [
        "Get user location",
        "Query: missions WHERE city = user_city",
        "For each mission, check current time against operatingHours",
        "Filter: isOpen = true AND currentTime in operatingHours",
        "Return list of currently open missions with emergency contact if outside hours"
      ]
    }
