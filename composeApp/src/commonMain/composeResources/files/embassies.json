  "geospatialSearchSchema": {
    "indexingRecommendation": "Use geographic hash indexing or R-tree spatial index for fast queries",
    "nearestNeighborQuery": {
      "description": "Find embassies/consulates within X km of user location",
      "algorithm": "Haversine distance formula",
      "formula": "a = sin²(Δφ/2) + cos φ1 ⋅ cos φ2 ⋅ sin²(Δλ/2); c = 2 ⋅ atan2( √a, √(1−a) ); d = R ⋅ c",
      "example": {
        "userLatitude": 28.6139,
        "userLongitude": 77.1987,
        "radiusKm": 5,
        "query": "Find all embassies within 5 km of user location"
      }
    },

    "metadataForIndexing": [
      "country_code_representing",
      "country_code_host",
      "mission_type",
      "latitude",
      "longitude",
      "city",
      "visa_services_available",
      "processing_time_average",
      "has_visa_interview_facility",
      "emergency_services_24_7"
    ],

    "searchFilters": {
